<head>
  <!-- Statistics display js-->
  <script src="https://cdn.jsdelivr.net/npm/chart.js" nonce="<%= nonce %>"></script>
</head>

<body>
  <div class="container-fluid">
    <div class="row justify-content-center mt-5 gapcontainer">
      <!-- User Statistics Container -->
      <div id="UserStatisticsContainer" class="col-md-3 bg-light p-4">
        <div class="chart-container">
          <canvas id="userStatisticsChart"></canvas>
        </div>
      </div>

      <!-- Project Statistics Container -->
      <div id="ProjectStatisticsContainer" class="col-md-3 bg-light p-4">
        <div class="chart-container">
          <canvas id="projectStatisticsChart"></canvas>
        </div>
      </div>

      <div id="ProjectStatusStatisticsContainer" class="col-md-3 bg-light p-4">
        <div class="chart-container">
          <canvas id="projectStatusStatisticsChart"></canvas>
        </div>
      </div>
    </div>
  </div>
  <script src="/js/Statistics.js" nonce="<%= nonce %>"></script>
</body>